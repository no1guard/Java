package com.kh.variable.pre;

public class EnumPre {
	//enum 무지개 {빨~보}; 빨주노초파노보
	public enum 무지개{
		빨간색,
		주황색,
		노란색,
		초록색,
		파란색,
		남색,
		보라색
		
	}public static void main(String[] args) {
		System.out.println(무지개.빨간색);
		System.out.println("색상 :"+무지개.주황색);
		System.out.println("색상 :"+무지개.노란색);
		System.out.println("색상 :"+무지개.초록색);
		System.out.println("색상 :"+무지개.파란색);
		System.out.println("색상 :"+무지개.남색);
		System.out.println("색상 :"+무지개.보라색);
	}
		//main
			//빨~보 출력
	
}

package com.kh.Exception;

import java.util.*;

/*
 Exception = 예외
 예외는 코드로 처리가능한 에러 
 
 */

public class Exception {
	
	private Scanner sc = new Scanner(System.in);

	//기본생성자
	public Exception() {
		 
	}
	 
	//메서드 2개 만들고
	public void method1() {
	// 		 index  0 1  2  3
		int[] 배열= {10,20,30,40};//마지막 인덱스가 3
		
		//for				 여기서 랭스는 4
		for(int i=0; i <배열.length; i++) {
			System.out.println(배열[i]);
			
		}
		
	}
	
	//메인메서드
	public static void main(String[] args) {
		Exception 예외발생 = new Exception();
		예외발생.method1();
		
	}
}

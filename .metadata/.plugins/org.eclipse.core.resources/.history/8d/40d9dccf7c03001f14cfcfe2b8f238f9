package com.kh.variable.pre;

public class EnumPre {
	//enum 무지개 {빨~보}; 빨주노초파노보
	public enum 무지개{
		빨간색,
		주황색,
		노란색,
		초록색,
		파란색,
		남색,
		보라색
		
	}
	

//메서드
	//void
	public void 메서드1() {
		 //장바구니 =배열에 담기
		//				enum 밑에있는 메서드 
		무지개[] 배열 =무지개.values(); //enum으로 작성한 값을 가지고옴
		//for-each
		for(무지개 무 : 배열 ) {
			System.out.println(무);
		}
	}
	
public static void main(String[] args) {
	EnumPre ep=new EnumPre();
	ep.메서드1();
}	
	 
}


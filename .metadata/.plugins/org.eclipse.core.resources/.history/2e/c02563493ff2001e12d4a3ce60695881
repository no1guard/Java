
public class Hello {

	public static void main(String[] args) {
		// System.out은 출력을 나타냅니다
		// print() 괄호 안에 전달된 값을 출력
        // ;(세미클론)은 코드에서 하나의 명령이 끝날 때마다 작성 
        System.out.print("안녕하세요");
        System.out.print("반갑습니다");
        System.out.println("환영합니다."); // println은 문장이 끝난 다음에 enter가 진행됩니다.
        System.out.println("어서오세요. 점심시간");
        System.out.println("1");//1로 출력됌
        System.out.println("2312321321");
        System.out.println("01234");
        System.out.println(2);
        System.out.println(01);//0을 앞에 넣으면 단순히 1로 출력이 됩니다 0을 앞에서 출력하고 싶으면 "" 큰 따옴표를 이용하여 출력하자
        System.out.println(011);
		System.out.println(true);//true=사실 맞다 		
		//단축키 ! 
		// sysout >> cltl + space >> 자동으로 System.out.println이 완성됌
		System.out.println(10);
		System.out.println(10);
		System.out.println(10);
	}

}

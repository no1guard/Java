package com.kh.variable.pre;

public class EnumPre {
	//enum 무지개 {빨~보}; 빨주노초파노보
	enum 무지개{
		빨간색,
		주황색,
		노란색,
		초록색,
		파란색,
		남색,
		보라색
		
	}
	
	//enum으로 소비자{학생,직장인,연장자}
	enum 소비자 {학생,직장인,연장자}
	//enum으로 판매자{소매업,도매업,중매업}
	enum 판매자 {소매업,도매업,중매업}
	
	
//생성자 필수 
	public EnumPre() {
		
	}

//메서드
	//void

	public void 메서드1() {
		 //장바구니 =배열에 담기
		//				enum 밑에있는 메서드 
		무지개[] 배열 =무지개.values(); //enum으로 작성한 값을 가지고옴
		//for-each
		for(무지개 무 : 배열 ) {
			System.out.println(무);
		}
	}
	
	public void 메서드2() {
		//소비자와 판매자를 for-each문 사용해서 각각 출력
		소비자[] 소비자1 = 소비자.values();
		판매자[] 판매자1 = 판매자.values();
		for(소비자 a : 소비자1) {
			System.out.println(a);
		}
		for(판매자 b : 판매자1) {
			System.out.println(b);
		}
		
		//소비자 학생 판매자 소매업 ordinal() if문 사용해서 비교하는 문장을 메서드 2 작성 
	}
public static void main(String[] args) {
	EnumPre ep=new EnumPre();
	ep.메서드2();
}	
	 
}


package com.kh.test.main;

import java.util.Scanner;

public class Test3 {
	public static void main(String[] args) {
		//Switch case 문 실행해보기 
		
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자 작성 : ");
		int 날짜 = sc.nextInt();
		String 할일;
		switch(날짜) {
		case 1: 
			할일 = "월요일 : 업무";
			break;
		case 2: 
			할일 = "화요일 : 공부";
			break;
		
		
		}
	}
}
